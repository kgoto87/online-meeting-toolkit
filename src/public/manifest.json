{
    "manifest_version": 3,
    "name": "Online Meeting Toolkit",
    "description": "Some handy tools for online meeting",
    "version": "0.1.0",
    "action": {
        "default_title": "Online Meeting Toolkit"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "resources": ["assets/musics/*.mp3"],
            "matches": ["<all_urls>"]
        }
    ]
}
